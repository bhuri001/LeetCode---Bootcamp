        if not head or not head.next:
           return True
        result = head
        new = []

        while result:
            new.append(result.val)
            result = result.next

        i=0
        j=len(new)-1

        while i<j:
            if new[i]==new[j]:
                i += 1
                j -= 1                
            else:
                return False

        return True
